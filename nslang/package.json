{
  "name": "nslang",
  "displayName": "nslang",
  "description": "nanoscript language service.",
  "version": "0.0.4",
  "publisher": "liamlangli",
  "scripts": {
    "pack": "vsce pack"
  },
  "engines": {
    "vscode": "^1.85.0"
  },
  "categories": [
    "Programming Languages",
    "Themes"
  ],
  "repository": {
    "type": "git",
    "url": "git@github.com:liamlangli/ns.git"
  },
  "icon": "./ns.png",
  "contributes": {
    "languages": [
      {
        "id": "ns",
        "icon": {
          "light": "./ns.svg",
          "dark": "./ns.svg"
        },
        "aliases": [
          "nanoscript",
          "ns"
        ],
        "extensions": [
          ".ns"
        ],
        "configuration": "./language-configuration.json"
      },
      {
        "id": "ns-markdown-injection"
      }
    ],
    "grammars": [
      {
        "language": "ns",
        "scopeName": "source.ns",
        "path": "./ns.json"
      },
      {
        "language": "ns-markdown-injection",
        "scopeName": "markdown.ns.codeblock",
        "path": "./markdown.json",
        "injectTo": [
          "text.html.markdown"
        ],
        "embeddedLanguages": {
          "meta.embedded.block.ns": "ns"
        }
      }
    ],
    "iconThemes": [
      {
        "id": "nslang",
        "label": "nslang icons",
        "path": "./icon.json"
      }
    ]
  }
}