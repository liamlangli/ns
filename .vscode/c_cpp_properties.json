{
    "configurations": [
        {
            "name": "MacARM",
            "includePath": [
                "${workspaceFolder}/include/**",
                "${workspaceFolder}/lsp/include/**",
                "/opt/homebrew/opt/llvm/include"
            ],
            "defines": ["NS_BITCODE", "NS_DEBUG", "NS_DARWIN"],
            "macFrameworkPath": [
                "/Library/Developer/CommandLineTools/SDKs/MacOSX14.sdk/System/Library/Frameworks"
            ],
            "compilerPath": "/opt/homebrew/opt/llvm/bin/clang",
            "cStandard": "c17",
            "cppStandard": "c++17",
            "intelliSenseMode": "macos-clang-arm64"
        },
        {
            "name": "MacX64",
            "includePath": [
                "${workspaceFolder}/include/**",
                "${workspaceFolder}/lsp/include/**",
                "/usr/local/opt/llvm/include"
            ],
            "defines": ["NS_BITCODE", "NS_DEBUG", "NS_DARWIN"],
            "macFrameworkPath": [
                "/Library/Developer/CommandLineTools/SDKs/MacOSX14.sdk/System/Library/Frameworks"
            ],
            "compilerPath": "/usr/local/opt/llvm/bin/clang",
            "cStandard": "c17",
            "cppStandard": "c++17",
            "intelliSenseMode": "macos-clang-arm64"
        }
    ],
    "version": 4
}