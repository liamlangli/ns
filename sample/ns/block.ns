import std

type op_fn = (i32, i32) -> i32

fn do_op(op: op_fn, a: i32, b: i32): i32 {
    return op(a, b)
}

fn main() {
    let add: op_fn = { a, b in
        return a + b
    }

    let sub = do_op({ a, b in
        return a - b
    }, 5, 3)

    let sum = (1 + 2) * 4 - 5

    let result = add(5, 3)
    print(`add result: {result}`)
    print(`sub result: {sub}`)
}